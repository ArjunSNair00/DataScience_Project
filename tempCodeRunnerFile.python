# %%
import numpy as np
import pandas as pd
#creating array from list
arr=[0,1,2,3,4]
s1=pd.Series(arr)
s1

# %%
#using randomness for values
n=np.random.randn(5) #random Ndarray
index=['a','b','c','d','e']
s2=pd.Series(n,index=index)
s2

# %%
#using dictionary
d={'a':1,'b':2,'c':3,'d':4,'e':5}
s3=pd.Series(d)
s3

# %%
#modifying the index of series
print(s1)
s1.index=['A','B','C','D','E']
s1

# %%
#slicing
a=s1[2:5]
print(a)


